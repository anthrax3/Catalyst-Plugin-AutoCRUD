<tr class="cpac_row[% ' cpac_stripe' IF stripe % 2 %]">
[% FOR col IN cpac.tc.cols %]
    [% NEXT IF cpac.tc.hidden_cols.exists(col) %]
    <td class="cpac_cell">
        [% PROCESS cell.tt
            value = item.$col
            meta  = cpac.tm.f.$col
        %]
    </td>
[% END %]
</tr>

[% SET stripe = stripe + 1 %]
